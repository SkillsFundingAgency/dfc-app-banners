<policies>
  <inbound>
    <base />
    <set-header name="Accept" exists-action="override">
      <value>application/json</value>
    </set-header>
    <set-backend-service base-url="__applicationUrl__" />
  </inbound>
  <backend>
    <base />
  </backend>
  <outbound>
    <base />
  </outbound>
  <on-error>
    <base />
  </on-error>
</policies>