{
    "APPINSIGHTS_INSTRUMENTATIONKEY": "",
    "Logging": {
      "LogLevel": {
        "Default": "Debug"
      },
      "ApplicationInsights": {
        "LogLevel": {
          "Microsoft": "Trace"
        }
      }
    },
    "AllowedHosts": "*",
    "Configuration": {
      "ApplicationName": "DFC-App-Banners",
      "CosmosDbConnections": {
        "SharedContent": {
          "AccessKey": "<cosmos db access key>",
          "EndpointUrl": "<cosmos db endpoint>",
          "DatabaseId": "dfc-app-banners",
          "CollectionId": "banners",
          "PartitionKey": "/PartitionKey"
        }
      }
    },
    "Policies": {
      "HttpCircuitBreaker": {
        "DurationOfBreak": "00:01:00",
        "ExceptionsAllowedBeforeBreaking": 3
      },
      "HttpRetry": {
        "BackoffPower": 2,
        "Count": 3
      }
    },
    "CmsApiClientOptions": {
      "Timeout": "00:00:30",
      "BaseAddress": "<content api endpoint>"
    },
    "SubscriptionSettings": {
      "SubscriptionServiceEndpoint": "<subscription funcapp url>",
      "ApiKey": "<apim key>",
      "Endpoint": "<banners app service url>/api/webhook/ReceiveEvents",
      "Filter": {
        "IncludeEventTypes": [ "published", "unpublished", "deleted" ],
        "PropertyContainsFilters": [
          {
            "Key": "subject",
            "Values": [
              "/content/banner/",
              "/content/pagebanner/"
            ]
          }
        ]
      }
    }
  }
  